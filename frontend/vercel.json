{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install --production=false",
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 60
    }
  }
}
